__NUXT_JSONP__("/studio", (function(a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z,A,B,C){return {data:[{story:{name:j,created_at:"2021-11-21T08:41:36.254Z",published_at:"2022-04-25T14:20:53.090Z",id:86906007,uuid:"bb5f7335-baeb-4674-a3aa-ece5d37cfbbd",content:{seo:{_uid:"e7dca6e2-1e6c-459d-afe6-e81ec954349f",title:j,plugin:"seo_metatags",og_image:b,og_title:j,description:l,twitter_image:b,twitter_title:j,og_description:l,twitter_description:l},_uid:"e24dbc72-d881-4a62-a49a-f046cc9121fa",hero:[{_uid:"d03eaeeb-e167-469d-b3b2-e6cd92b9117a",body:{type:f,content:[{type:"heading",attrs:{level:3},content:[{text:l,type:a}]},{type:d,content:[{text:"For over a decade our team has had the privilege of working with global brands and inspiring startups to tell brand-led stories and shape digital identities.",type:a}]}]},link:[{_uid:"9735774a-de6e-4649-a256-5b7d3e9bafd2",link:{id:b,url:w,linktype:m,fieldtype:"multilink",cached_url:w},text:"Our Story",image:{id:c,alt:c,name:b,focus:c,title:c,filename:c,copyright:c,fieldtype:g},component:"TextLink"}],image:{id:3685171,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F2500x2626\u002F63a902ce2b\u002Fexo-ape-studio-hero.jpg",copyright:b,fieldtype:g},title:"Building\nDigital\nBrands",subtitle:"Through elegant and refined digital experiences",component:"StudioHero"}],sections:[{_uid:"f3fef2c9-d999-4731-8d75-05a4532a7e32",body:{type:f,content:[{type:x,content:[{type:e,content:[{type:d,content:[{text:"Art Direction",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Animation & Motion",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Branding & Indentity",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Content Production",type:a}]}]},{type:e,content:[{type:d,content:[{text:"User Interface Design",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Creative Development",type:a}]}]},{type:e,content:[{type:d,content:[{text:"E-Commerce",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Front-End Development",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Back-End Development",type:a}]}]},{type:e,content:[{type:d,content:[{text:"Progressive Web Apps",type:a}]}]}]}]},label:"Our Services",title:"Digital\nInnovation",images:[{id:3685164,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F1500x2123\u002F637f850b2b\u002Fexo-ape-studio-collage-1.jpg",copyright:b,fieldtype:g},{id:3685166,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F1500x1003\u002F2581b1d31d\u002Fexo-ape-studio-collage-2.jpg",copyright:b,fieldtype:g},{id:3685170,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F569x809\u002Fdd3f38466e\u002Fexo-ape-studio-collage-3.jpg",copyright:b,fieldtype:g}],component:"StudioServices"},{_uid:"53bb95a7-035b-4074-912b-ad02d542c733",body:{type:f,content:[{type:d,content:[{text:"We believe that all good things are achieved by those who are willing to put in passion, courage and craftsmanship.",type:a}]}]},items:[{_uid:"d5526ef1-5ec5-4f4c-bc1c-9c77c526d5b4",body:{type:f,content:[{type:d,content:[{text:"Especially in today’s fast-paced digital landscape the first impression is the one that matters most. We design remarkable experiences that are easy-to-use, yet impossible to forget. Through profound design aesthetics, meticulously crafted details and surprising interactions we create moments of delight that go straight to the soul.",type:a}]},{type:d,content:[{text:"“People may forget what you said or what you did, but never how you made them feel.”",type:a}]}]},title:"Design for Emotions",component:n},{_uid:"5e073b44-f127-4f81-a9ed-a121f72dcf26",body:{type:f,content:[{type:d,content:[{text:"Aesthetics work, but not without a strong foundation. Our creative concepts are backed by an insight-based approach and thorough understanding of your brand. This fuels us with the knowledge to strategically solve even the most complex problems. Only then, we can create digital experiences that maintain a yin-yang balance between magic and logic.",type:a}]}]},title:"Backed  by Reason",component:n},{_uid:"2418f90d-fd4e-46bc-9011-2d6a917f142f",body:{type:f,content:[{type:d,content:[{text:"Now more than ever, we believe that digital should be at the core of every forward-thinking brand. We connect the physical with the digital through online platforms, mobile and e-commerce solutions that stand out through captivating visuals, branded interactivity and cutting-edge performance.",type:a}]}]},title:"Digital-first Approach",component:n},{_uid:"929bf572-d543-4426-a93a-99fd11709f41",body:{type:f,content:[{type:d,content:[{text:"At the end of the day we’re humans, and we’d like to keep it that way. Positivity wins. Compassion and honesty guide us in our day-to-day communications. We pour a lot of soul and passion in our work. humble, but proud of what we do—without the chest banging.",type:a}]}]},title:"Authenticity at the Core",component:n}],label:"How we work",title:"Forever Upwards",component:"StudioHowWeWork"},{_uid:"e40531c8-a5f1-477a-8508-13454d52d2ac",body:{type:f,content:[{type:d,content:[{text:"For us, a digital product has got to be: simple, intuitive and layered from the core up. Profound design aesthetics complement a rock-solid concept that withstands the test of time. It has to stick, convert and inspire.",type:a}]},{type:d,content:[{text:"But most of all, function as a medium ",type:a,marks:[{type:s}]},{type:i,marks:[{type:s}]},{text:"to convey your brand’s story.",type:a,marks:[{type:s}]}]}]},image:{id:3685182,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F2500x1667\u002F5224da023b\u002Fexo-ape-studio-mood.jpg",copyright:b,fieldtype:g},label:"What we believe",quote:"Great digital products should find a harmonious balance between aesthetics and functionality.",title:"Data \nInforms,\n— Emotion\nDecides",component:"StudioWhatWeBelieve"},{_uid:"5ba7ceca-e124-4636-a466-4ca52f0c8ea3",body:{type:f,content:[{type:d,content:[{text:"We are a team* of like-minded design enthusiasts and tech aficionados that explore the digital frontier with grit and dedication. Intrigued by beauty, fascinated by technology and fuelled with an everlasting devotion to digital craftsmanship and meaningful aesthetics.",type:a}]}]},label:"The Apes",quote:"*We believe in a fluid team approach that allows us to bring together the best designers, developers and agencies in the world in order to serve the needs of today’s clients.",title:"Design &\nTechnology\nEnthusiasts",images:[{id:3685263,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F1920x1280\u002Fb13859fdad\u002Fexo-ape-studio-mood-team-04.jpg",copyright:b,fieldtype:g},{id:3685261,alt:h,name:b,focus:c,title:b,filename:"https:\u002F\u002Fa.storyblok.com\u002Ff\u002F133769\u002F1200x1790\u002Fac2f6934ee\u002Fexo-ape-studio-mood-team-03.jpg",copyright:b,fieldtype:g}],component:"StudioTheApes"},{_uid:"99c8b648-bdef-4ddd-9e20-b73f602c6512",body:{type:f,content:[{type:x,content:[{type:e,content:[{type:d,content:[{text:"Studio of the year ",type:a},{type:i},{text:"Nominee '20 ",type:a},{type:i},{text:t,type:a,marks:[{type:u,attrs:{href:"https:\u002F\u002Fwww.awwwards.com\u002F",uuid:c,anchor:c,target:v,linktype:m}}]}]}]},{type:e,content:[{type:d,content:[{text:"Ali Ali ",type:a},{type:i},{text:"Site of the Month",type:a},{type:i},{text:t,type:a,marks:[{type:u,attrs:{href:"https:\u002F\u002Fwww.awwwards.com\u002Fali-ali-by-exo-ape-wins-site-of-the-month-august-2020.html",uuid:c,anchor:c,target:v,linktype:m}}]}]}]},{type:e,content:[{type:d,content:[{text:"Ottografie ",type:a},{type:i},{text:"Photography site of the Year",type:a},{type:i},{text:y,type:a,marks:[{type:u,attrs:{href:"https:\u002F\u002Fwww.cssdesignawards.com\u002F",uuid:c,anchor:c,target:v,linktype:m}}]}]}]}]}]},label:"Recognition",title:"Awards &\nPublic